<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/validador/db" VERSION="20241212" COMMENT="XMLDB file for Moodle rss_client block"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLE NAME="block_validador_results" COMMENT="Resultados de las validaciones">
    <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
    <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
    <FIELD NAME="validationname" TYPE="char" LENGTH="255" NOTNULL="true" DEFAULT=""/>
    <FIELD NAME="passed" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0"/>
    <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
    <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
    
    <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
    <KEY NAME="course_validation_uniq" TYPE="unique" FIELDS="courseid, validationname"/>
  </TABLE>
</XMLDB>